{
  "name": "platform-backend",
  "version": "1.161.5",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "engines": {
    "node": ">=20.10.0 <21",
    "pnpm": ">=8.13.1 <9"
  },
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "cli": "tsx -r dotenv/config src/index.ts server start",
    "build": "node esbuilder.mjs",
    "dev": "prisma generate && NODE_OPTIONS=\"--require dotenv/config\" tsx watch --inspect src/index.ts server start",
    "start": "node --enable-source-maps -r dotenv/config dist/platform.js server start",
    "test": "TZ=UTC vitest run",
    "test:ci": "vitest run --coverage --outputFile.junit=./coverage/junit.xml --reporter=junit --coverage.reporter=lcov",
    "test:coverage": "vitest run --coverage --reporter=junit",
    "test:watch": "vitest",
    "lint": "pnpm run lint:eslint",
    "lint:fix": "eslint --fix --ignore-path .eslintignore './**/*.{ts,tsx}' && prisma format",
    "lint:fix:staged": "eslint --fix --ignore-path .eslintignore && prisma format",
    "lint:eslint": "eslint --ignore-path .eslintignore './**/*.{ts,tsx}' --quiet",
    "prepare": "husky || true",
    "release": "semantic-release",
    "db:migrate": "prisma migrate deploy",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/gitlab": "13.1.0",
    "@total-typescript/shoehorn": "0.1.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/http-errors": "2.0.4",
    "@types/lodash.debounce": "4.0.9",
    "@types/luxon": "3.4.2",
    "@types/node": "20.14.2",
    "@types/pg": "8.11.6",
    "@types/semver": "7.5.8",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@vitest/coverage-v8": "1.6.0",
    "conventional-changelog-angular": "8.0.0",
    "dotenv": "16.4.5",
    "esbuild": "0.21.4",
    "esbuild-node-externals": "1.13.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "53.0.0",
    "eslint-plugin-vitest": "0.5.4",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "msw": "2.3.1",
    "prettier": "3.3.1",
    "rimraf": "5.0.7",
    "semantic-release": "24.0.0",
    "semantic-release-slack-bot": "4.0.2",
    "ts-node": "10.9.2",
    "tsc-files": "1.1.4",
    "tsx": "4.13.2",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@fastify/auth": "4.6.1",
    "@fastify/jwt": "8.0.1",
    "@fastify/multipart": "8.3.0",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "@opentelemetry/api": "1.9.0",
