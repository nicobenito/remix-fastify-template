    "lodash.debounce": "4.0.8",
    "luxon": "3.4.4",
    "pg": "8.12.0",
    "pino": "9.1.0",
    "pino-pretty": "11.2.0",
    "prisma": "5.14.0",
    "semver": "7.6.2",
    "ts-pattern": "5.1.2",
